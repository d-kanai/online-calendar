{
  "name": "online-calendar-backend",
  "version": "1.0.0",
  "description": "Online Calendar Backend API with Hono",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "hono",
    "typescript",
    "api",
    "calendar"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@prisma/client": "^5.0.0",
    "hono": "^4.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@quramy/prisma-fabbrica": "^2.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prisma": "^5.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
