generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Meeting {
  id        String   @id @default(cuid())
  title     String
  startTime DateTime
  endTime   DateTime
  isImportant Boolean @default(false)
  ownerId   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("meetings")
}